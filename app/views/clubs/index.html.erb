<h1>London Clubs</h1>

<table>
  <tr>
    <th>Club</th>
    <th>Website</th>
    <th>Contact</th>
    <th>location_lat</th>
    <th>location_lng</th>
    <th>postcode</th>
  </tr>

  <% @clubs.each do |club| %>
    <tr>
      <td><%= club.name %></td>
      <td><%= club.website %></td>
      <td><%= club.contact %></td>

      <td><%= link_to 'Show', club_path(club) %></td>
      <td><%= link_to 'Edit', edit_club_path(club) %></td>
      <td><%= link_to 'Destroy', club_path(club),
        method: :delete,
        data: { confirm: 'Delete this club?' } %></td>
    </tr>
  <% end %>
</table>

<br>
<%= link_to 'Add club', new_club_path %>
