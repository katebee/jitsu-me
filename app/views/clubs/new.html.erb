<h1>New Club</h1>

<%= form_for :club do |f| %>
  <p>
    <%= f.label :club_name %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :website %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :contact %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
