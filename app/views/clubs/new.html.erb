<h1>New Club</h1>

<%= form_for :club, url: clubs_path do |f| %>
  <p>
    <%= f.label :name %><br>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :website %><br>
    <%= f.text_field :website %>
  </p>

  <p>
    <%= f.label :contact %><br>
    <%= f.text_area :contact %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
