<h1>Listing Events</h1>

<br>

<div id="calendar"></div>

<br>

<h3>Details</h3>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Start time</th>
      <th>End time</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessions.each do |session| %>
      <tr>
        <td><%= session.title %></td>
        <td><%= session.description %></td>
        <td><%= session.start_time %></td>
        <td><%= session.end_time %></td>
      </tr>
    <% end %>
  </tbody>
</table>
